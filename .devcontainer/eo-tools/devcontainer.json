{
  "name": "eo_tools",
  "dockerComposeFile": "../../docker-compose.yml",
  "service": "eo_tools_service",
  "workspaceFolder": "/app",
  "mounts": [
  "source=/mnt/x/library/tomwilsonsco/eo_tools,target=/app,type=bind"
  ],
  "forwardPorts": [
    "localhost:8085"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        // "ms-python.vscode-pylance",
        // "ms-python.pylint",
        // "ms-python.flake8",
        "ms-python.isort",
        // "ms-python.autopep8",
        "ms-python.black-formatter",
        "ms-toolsai.jupyter",
        "njpwerner.autodocstring"
      ],
      "settings": {
        "git.ignoreLimitWarning": true,
        // "python.formatting.provider": "none",
        "[python]": {
          "editor.defaultFormatter": "ms-vscode.vscode-js-profile-table",
          "editor.formatOnSave": true
        }
      }
    }
  }
}